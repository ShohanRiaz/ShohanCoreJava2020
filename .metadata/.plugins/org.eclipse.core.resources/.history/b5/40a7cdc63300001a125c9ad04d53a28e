package simpleTest;

public class James2 {

		public String Name; 
		public char grade;
		public int id;
		public boolean female;

		public James2() { 
			System.out.println("In parameterized constructor, we can initialize as much data as we can, like:"); 
	


		}
	
		public James2(String Name,  char grade, int id, boolean female) { 
			this.Name = Name;
			this.grade = grade;
			this.id = id;		
			this.female = female;
		
			
		}
		
			public void studentName() {

			}
			public static void main(String[] args) {
			James2 student1 = new James2();
			student1.studentName();
			James2 student2 = new James2("James", 'C', 22342, false); 
			char[] James2 = null;
			System.out.println(James2);
					}
}
